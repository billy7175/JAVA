package B3_연산자_기본이론;

public class B3_기본이론2_짝수홀수 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 배수 구하는법
		// 2의 배수를 구하고 싶다.
		// 어떤 수를 2로 나눠서 나머지가 0이면 2의 배수
		
		int a = 10;
		System.out.println(a % 2 == 0);
		int b = 11;
		System.out.println(b % 3 == 0);
	}

}
