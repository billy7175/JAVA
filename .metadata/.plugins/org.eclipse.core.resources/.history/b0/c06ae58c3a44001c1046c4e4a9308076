package B1_변수_기본이론;

public class B1_기본이론1_변수 {
	/*
	 * # 변수(variable)
	 * 1. 변수란, 단 하나의 값을 저장할 수 있는 메모리 공간
	 * 2. 변수의 선언 : int a;		// 메모리 공간 할당
	 * 3. 변수의 초기화 : a = 10;	// 값 저장
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 정수 , 실수 , 한글자 , 여러글자
		// 1. 정수
		int a;
		int b = 13;
		a = 10;
				System.out.println(a);
				System.out.println(b);
	}

}
