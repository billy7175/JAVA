package B1_변수_기본이론;

public class B1_기본이론6_변수의크기와종류 {
	public static void main(String[] args) {
		// 정수의 종류는 여러가지
		// int 의 크기는 -20억 ~ + 20억 사이이다.
		// 정수는 주로 int를 사용한다.
		int a;
		
		 // a = 0000000000000
		 long l;
		 l = 3000000000000000L;
		 System.out.println(l);
		 
		 // 실수도 종류가 여러가지이다.
		 float f = 10.123123123123123F;
		 double d = 10.123123123123123;
		 System.out.println(f);
		 System.out.println(d);
	}
}
