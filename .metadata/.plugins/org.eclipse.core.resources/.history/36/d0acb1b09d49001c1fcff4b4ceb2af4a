package 복습;

public class 복습3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 민수 40%(16표); 영희(9표); 철수 나머지
		double 전체 = 40;
		double 일퍼센트 = 전체 / 100;
		double 민수 = 일퍼센트 * 40;
		
		System.out.println("일퍼센트");
		System.out.println("1% = " + 일퍼센트);
		System.out.println("민수 = " + 민수 + " 표");
		
//		double 전체 = 40;
//		double 일 = 전체/100;
//		double 민수 = 일 * 40;
//		System.out.println(민수);
//		double 남은수 = 전체 - 민수;
//		System.out.println(남은수);
//		남은수 = 남은수 - 9;
//		System.out.println(남은수);
//		double 확률 = 남은수/일;
//		System.out.println(확률);
	}

}
